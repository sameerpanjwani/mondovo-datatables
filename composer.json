{
    "name": "mondovo/datatable",
    "description": "Customized version of laravel Data Table. ",
    "authors": [
        {
            "name": "Jestins",
            "email": "jestin@maximizer-e-services.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/config": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/routing": "^7.0",
        "illuminate/session": "^7.0",
        "illuminate/support": "^7.0",
        "yajra/laravel-datatables-oracle": "~9.0",
        "maatwebsite/excel": "^3.1",
        "nlp-tools/nlp-tools": "~0.1.1"
    },
    "autoload-dev": {
        "classmap": [
            "tests/DrawTableFilterTest.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "Mondovo\\DataTable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mondovo\\DataTable\\MondovoDataTableServiceProvider"
            ],
            "aliases": {
                "Excel": "Maatwebsite\\Excel\\Facades\\Excel"
            }
        },
        "minimum-stability": "dev",
        "prefer-stable": true
    }
}
